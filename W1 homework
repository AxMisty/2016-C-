#include<iostream>  
#include<string>

using namespace std;

int main()
{
	cout<<"请输入您的姓名：";
	string name;
	cin>>name;
	cout<<"您好，"+name+"!"<<endl;
	cout<<"请输入您的编号、性别('M'表示男性，'F'表示女性)、年龄、利手('R'表示右手，'L'表示左手):"<<endl;
	int number,age;
	char gender,hand;
	cin>>number>>gender>>age>>hand;
	cout<<endl<<"（实验指导语）";
	return 0; 
}
